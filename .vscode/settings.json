{
  "explorer.fileNesting.enabled": false,
  "explorer.fileNesting.expand": false,
  "explorer.fileNesting.patterns": {
    // "vue3.ts": "vue*.ts",
    // "package.json": ".npmrc, pnpm-lock.yaml,  pnpm-workspace.yaml",
    // ".gitignore": ".gitmodules",
    // ".eslintrc.js": "commitlint.config.js, stylelint.config.js",
  },
  "eslint.workingDirectories": [
    {
      "mode": "auto"
    }
  ],
  "files.associations": {
      "*.json": "jsonc"
  },
  "better-comments.highlightPlainText": true,
  "better-comments.tags": [
    {
      "tag": "??",
      "color": "#E25530",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "todo",
      "color": "#FF8C00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "*",
      "color": "#98C379",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
    {
      "tag": "!",
      "color": "#FF2D00",
      "strikethrough": false,
      "underline": false,
      "backgroundColor": "transparent",
      "bold": false,
      "italic": false
    },
  ]
}
